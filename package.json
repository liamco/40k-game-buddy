{
    "name": "@figma/my-make-file",
    "private": true,
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "build": "vite build",
        "dev": "vite --port 4000",
        "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
        "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
        "lint:format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
        "parse-data": "npm run _parse-depot-data && npm run _migrate-abilities && npm run _parse-datasheet-fields && npm run _parse-wargear-options",
        "_parse-depot-data": "node scripts/regex-parsers/parse-depot-data.js",
        "_migrate-abilities": "node scripts/regex-parsers/migrate-abilities.js",
        "_parse-datasheet-fields": "node scripts/regex-parsers/parse-datasheet-fields.js",
        "_parse-wargear-options": "node scripts/regex-parsers/parse-wargear-options.js",
        "extract-effects:regex": "node scripts/regex-parsers/extract-mechanics-regex.js",
        "extract-effects:openai": "node scripts/openai-extractors/extract-effects.js",
        "clear-mechanics": "node scripts/regex-parsers/clear-mechanics.js",
        "generate-loadouts": "node scripts/regex-parsers/generate-valid-loadouts.js",
        "test:combat": "node scripts/test-combat-engine.js"
    },
    "dependencies": {
        "@radix-ui/react-accordion": "1.2.3",
        "@radix-ui/react-alert-dialog": "1.1.6",
        "@radix-ui/react-aspect-ratio": "1.1.2",
        "@radix-ui/react-avatar": "1.1.3",
        "@radix-ui/react-checkbox": "1.1.4",
        "@radix-ui/react-collapsible": "1.1.3",
        "@radix-ui/react-context-menu": "2.2.6",
        "@radix-ui/react-dialog": "1.1.6",
        "@radix-ui/react-dropdown-menu": "2.1.6",
        "@radix-ui/react-hover-card": "1.1.6",
        "@radix-ui/react-label": "2.1.2",
        "@radix-ui/react-menubar": "1.1.6",
        "@radix-ui/react-navigation-menu": "1.2.5",
        "@radix-ui/react-popover": "1.1.6",
        "@radix-ui/react-progress": "1.1.2",
        "@radix-ui/react-radio-group": "1.2.3",
        "@radix-ui/react-scroll-area": "1.2.3",
        "@radix-ui/react-select": "2.1.6",
        "@radix-ui/react-separator": "1.1.2",
        "@radix-ui/react-slider": "1.2.3",
        "@radix-ui/react-slot": "1.1.2",
        "@radix-ui/react-switch": "1.1.3",
        "@radix-ui/react-tabs": "1.1.3",
        "@radix-ui/react-toggle": "1.1.2",
        "@radix-ui/react-toggle-group": "1.1.2",
        "@radix-ui/react-tooltip": "1.1.8",
        "class-variance-authority": "0.7.1",
        "classnames": "^2.5.1",
        "clsx": "2.1.1",
        "lucide-react": "0.487.0",
        "react-router-dom": "^7.12.0",
        "sonner": "2.0.3",
        "tailwind-merge": "3.2.0",
        "tw-animate-css": "^1.4.0"
    },
    "devDependencies": {
        "@tailwindcss/vite": "4.1.12",
        "@types/node": "^25.0.10",
        "@vitejs/plugin-react": "4.7.0",
        "dotenv": "^17.2.3",
        "prettier": "^3.2.5",
        "tailwindcss": "4.1.12",
        "vite": "6.3.5"
    },
    "imports": {
        "#data/*": "./src/app/data/*",
        "#assets/*": "./src/app/assets/*",
        "#components/*": "./src/app/components/*",
        "#modules/*": "./src/app/modules/*",
        "#scripts/*": "./scripts/*",
        "#types/*": "./src/app/types/*"
    },
    "peerDependencies": {
        "react": "18.3.1",
        "react-dom": "18.3.1"
    },
    "peerDependenciesMeta": {
        "react": {
            "optional": true
        },
        "react-dom": {
            "optional": true
        }
    },
    "pnpm": {
        "overrides": {
            "vite": "6.3.5"
        }
    }
}
