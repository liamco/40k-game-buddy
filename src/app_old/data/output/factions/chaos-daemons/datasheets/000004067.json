  "options": [
    {
      "datasheetId": "000004067",
      "line": 1,
      "button": "•",
      "description": "The Raptor Champion’s bolt pistol can be replaced with 1 plasma pistol."
    },
    {
      "datasheetId": "000004067",
      "line": 2,
      "button": "•",
      "description": "The Raptor Champion’s Astartes chainsword can be replaced with one of the following:<ul><li>1 accursed weapon</li><li>1 heavy melee weapon</li></ul>"
    },
    {
      "datasheetId": "000004067",
      "line": 3,
      "button": "•",
      "description": "For every 5 models in this unit, up to 2 Raptors can each have their bolt pistol replaced with 1 plasma pistol (these models’ Astartes chainswords cannot be replaced)."
    },
    {
      "datasheetId": "000004067",
      "line": 4,
      "button": "•",
      "description": "For every 5 models in this unit, up to 2 Raptors can each have their Astartes chainsword replaced with 1 heavy melee weapon."
    },
    {
      "datasheetId": "000004067",
      "line": 5,
      "button": "•",
      "description": "For every 5 models in this unit, 1 Raptor can replace their Astartes chainsword with 1 mutations."
    },
    {
      "datasheetId": "000004067",
      "line": 6,
      "button": "•",
      "description": "Up to 2 Raptors can each have their Astartes chainsword replaced with one of the following options (you cannot select the same option more than once):<ul><li>1 flamer and 1 close combat weapon</li><li>1 meltagun and 1 close combat weapon</li><li>1 plasma gun and 1 close combat weapon</li></ul>"
    },
    {
      "datasheetId": "000004067",
      "line": 7,
      "button": "•",
      "description": "If this unit contains 10 models, up to 2 additional Raptors can each have their Astartes chainsword replaced with one of the following options (you cannot select the same option more than once):<ul><li>1 flamer and 1 close combat weapon</li><li>1 meltagun and 1 close combat weapon</li><li>1 plasma gun and 1 close combat weapon</li></ul>"
    }
  ],
