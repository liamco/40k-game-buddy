  "options": [
    {
      "datasheetId": "000003824",
      "line": 1,
      "button": "•",
      "description": "Any number of Kill Team Veterans can replace their boltgun and Long Vigil melee weapon with:<ul><li>1 Long Vigil ranged weapon</li><li>1 boltgun and 1 Astartes shield</li><li>1 Long Vigil melee weapon and 1 Astartes shield</li></ul>"
    },
    {
      "datasheetId": "000003824",
      "line": 2,
      "button": "•",
      "description": "For every 5 models in this unit, up to 2 models can replace their boltgun and Long Vigil melee weapon with 1 Deathwatch thunder hammer."
    },
    {
      "datasheetId": "000003824",
      "line": 3,
      "button": "•",
      "description": "For every 5 models in the unit, up to 2 models can replace their boltgun and Long Vigil melee weapon with one of the following:<ul><li>1 frag cannon</li><li>1 Infernus heavy bolter</li></ul>"
    },
    {
      "datasheetId": "000003824",
      "line": 4,
      "button": "•",
      "description": "Any number of Kill Team Biker models can be equipped with one of the following:<ul><li>1 bolt pistol</li><li>1 Long Vigil melee weapon</li></ul>"
    },
    {
      "datasheetId": "000003824",
      "line": 5,
      "button": "•",
      "description": "For every 5 models in this unit, up to 2 models  can replace their boltgun and Long Vigil melee weapon with 1 Deathwatch thunder hammer."
    },
    {
      "datasheetId": "000003824",
      "line": 6,
      "button": "•",
      "description": "Up to 3 models can each have their storm bolter replaced with one of the following:<ul><li>1 assault cannon</li><li>1 heavy flamer</li><li>1 plasma cannon</li><li>1 cyclone missile launcher and 1 storm bolter (this model's storm bolter cannot be replaced)</li></ul>"
    },
    {
      "datasheetId": "000003824",
      "line": 7,
      "button": "•",
      "description": "Any number of Kill Team Terminator models can replace their power fist and storm bolter with one of the following:<ul><li>1 storm bolter and 1 power weapon</li><li>1 storm bolter and 1 chainfist</li><li>1 twin lightning claws</li><li>1 Terminator thunder hammer and 1 storm shield</li></ul>"
    }
  ],
