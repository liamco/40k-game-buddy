  "options": [
    {
      "datasheetId": "000003950",
      "line": 1,
      "button": "•",
      "description": "For every 10 models in this unit, up to 2 Death Korps Troopers can each have their lasgun replaced with one of the following*:<ul><li>1 flamer</li><li>1 grenade launcher</li><li>1 long-las</li><li>1 meltagun</li><li>1 plasma gun</li></ul>"
    },
    {
      "datasheetId": "000003950",
      "line": 2,
      "button": "•",
      "description": "For every 10 models in this unit, 1 Death Korps Trooper equipped with a lasgun can be equipped with 1 Death Korps medi-pack (that model’s lasgun cannot be replaced)**."
    },
    {
      "datasheetId": "000003950",
      "line": 3,
      "button": "•",
      "description": "For every 10 models in this unit, 1 Death Korps Trooper equipped with a lasgun can be equipped with 1 vox-caster (that model’s lasgun cannot be replaced).**"
    },
    {
      "datasheetId": "000003950",
      "line": 4,
      "button": "•",
      "description": "Any number of Death Korps Watchmasters can each replace their laspistol and chainsword with 1 boltgun and 1 close combat weapon."
    },
    {
      "datasheetId": "000003950",
      "line": 5,
      "button": "•",
      "description": "Any number of Death Korps Watchmasters can each replace their chainsword with 1 power weapon."
    },
    {
      "datasheetId": "000003950",
      "line": 6,
      "button": "•",
      "description": "Any number of Death Korps Watchmasters can each replace their laspistol with one of the following:<ul><li>1 bolt pistol</li><li>1 plasma pistol</li></ul>"
    },
    {
      "datasheetId": "000003950",
      "line": 7,
      "button": "*",
      "description": "*  You cannot select the same option more than once per unit unless it contains 20 models, in which case you cannot select the same option more than twice per unit."
    },
    {
      "datasheetId": "000003950",
      "line": 8,
      "button": "*",
      "description": "** A model can only take one of these options."
    }
  ],
