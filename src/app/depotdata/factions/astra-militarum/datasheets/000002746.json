  "options": [
    {
      "datasheetId": "000002746",
      "line": 1,
      "button": "•",
      "description": "For every 5 models in this unit, up to two Tempestus Scions can each have their hot-shot lasgun replaced with one of the following*:<ul><li>1 flamer</li><li>1 grenade launcher</li><li>1 hot-shot volley gun</li><li>1 meltagun</li><li>1 plasma gun</li></ul>"
    },
    {
      "datasheetId": "000002746",
      "line": 2,
      "button": "•",
      "description": "1 Tempestus Scion’s hot-shot lasgun can be replaced with 1 hot-shot laspistol and 1 vox-caster."
    },
    {
      "datasheetId": "000002746",
      "line": 3,
      "button": "•",
      "description": "The Tempestor’s chainsword can be replaced with one of the following:<ul><li>1 power fist</li><li>1 power weapon</li></ul>"
    },
    {
      "datasheetId": "000002746",
      "line": 4,
      "button": "•",
      "description": "The Tempestor’s hot-shot laspistol can be replaced with one of the following:<ul><li>1 bolt pistol</li><li>1 plasma pistol</li></ul>"
    },
    {
      "datasheetId": "000002746",
      "line": 5,
      "button": "*",
      "description": "* You cannot select the same weapon from this list more than once per unit, unless this unit contains 10 models, in which case you cannot select the same weapon more than twice  per unit."
    }
