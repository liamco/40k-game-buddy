  "abilities": [
    {
      "id": "000008337",
      "name": "Stealth",
      "legend": "Some warriors are masters of disguise and concealment.",
      "factionId": "",
      "description": "<div class=\"Corner33 abWrap BreakInsideAvoid\"><div class=\"abFlex\"><div class=\"abDiamondWrap\"><div class=\"abDiamond\"><div class=\"abIcon\"></div></div></div><div class=\"abLine\"></div><div class=\"abNameWrap\"></div></div><p class=\"ShowFluff stratLegend2\">Some warriors are masters of disguise and concealment.</p>If every model in a unit has this ability, then each time a ranged attack is made against it, subtract 1 from that attack’s <span class=\"tooltip00014\" data-tooltip-content=\"#tooltip_content00014\"><span class=\"tt kwbu\">Hit</span> <span class=\"tt kwbu\">roll</span></span>.</div>",
      "type": "Core"
    },
    {
      "id": "",
      "name": "Visions of Suffering (Psychic)",
      "legend": "",
      "factionId": "",
      "description": "Each time a model in this unit makes an attack that targets an enemy unit that is below its Starting Strength, add 1 to the Hit roll. If that enemy unit is Below Half-strength, add 1 to the Wound roll as well.",
      "type": "Datasheet",
      "parameter": ""
    },
    {
      "id": "",
      "name": "Voice Eater",
      "legend": "",
      "factionId": "",
      "description": "Enemy units (excluding <span class=\"kwb\">MONSTERS</span> and <span class=\"kwb\">VEHICLES</span>) cannot be targeted with Stratagems while they are within Engagement Range of the bearer’s unit.",
      "type": "Wargear",
      "parameter": ""
    },
    {
      "id": "",
      "name": "ATTACHED UNIT",
      "legend": "",
      "factionId": "",
      "description": "If a <span class=\"tooltip00323\" data-tooltip-content=\"#tooltip_content00323\" data-tooltip-anchor=\"#tooltip_content00323\"><span class=\"kwb\">CHARACTER</span></span> unit from your army with the Leader ability (excluding <span class=\"tooltip00492\" data-tooltip-content=\"#tooltip_content00492\" data-tooltip-anchor=\"#tooltip_content00492\"><span class=\"kwb\">EPIC</span> <span class=\"kwb\">HERO</span></span>es) can be attached to a <span class=\"kwb kwbo\">LEGIONARIES</span> unit, it can be attached to this unit instead.",
      "type": "Special (правая колонка)",
      "parameter": ""
    }
  ],
