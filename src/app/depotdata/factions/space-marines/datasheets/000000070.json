{
  "id": "000000070",
  "name": "Tactical Squad",
  "factionId": "SM",
  "sourceId": "000000139",
  "legend": "Tactical Squads have formed the backbone of Space Marine Chapters for ten thousand years. With access to a wide range of weapons, they can purge swathes of enemy infantry, scour battlefields clean of heavily armoured foes or punch burning holes through tanks.",
  "role": "Battleline",
  "loadout": "<b>Every model is equipped with:</b> bolt pistol; boltgun; close combat weapon.",
  "transport": "",
  "virtual": false,
  "leaderHead": "",
  "leaderFooter": "",
  "damagedW": "",
