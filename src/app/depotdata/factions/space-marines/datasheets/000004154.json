  "options": [
    {
      "datasheetId": "000004154",
      "line": 1,
      "button": "•",
      "description": "The Sword Brother’s bolt pistol and boltgun can be replaced with 1 twin lightning claws or two different weapons from the following list*:<br><ul><li>1 Astartes chainsword</li><li>1 bolt pistol</li><li>1 boltgun</li><li>1 combi-weapon</li><li>1 grav-pistol</li><li>1 hand flamer</li><li>1 inferno pistol</li><li>1 plasma pistol</li><li>1 power fist</li><li>1 power weapon</li><li>1 storm bolter</li><li>1 thunder hammer</li></ul>"
    },
    {
      "datasheetId": "000004154",
      "line": 2,
      "button": "•",
      "description": "Any number of Neophytes can each have their boltgun replaced with one of the following: <br><ul><li>1 Astartes shotgun</li><li>1 combat knife</li></ul>"
    },
    {
      "datasheetId": "000004154",
      "line": 3,
      "button": "•",
      "description": "Any number of Initiates can each have their boltgun replaced with 1 Astartes chainsword."
    },
    {
      "datasheetId": "000004154",
      "line": 4,
      "button": "•",
      "description": "1 Initiate’s boltgun can be replaced with one of the following:<br><ul><li>1 flamer</li><li>1 plasma gun</li><li>1 meltagun</li><li>1 grav-gun</li></ul>"
    },
    {
      "datasheetId": "000004154",
      "line": 5,
      "button": "•",
      "description": "1 Initiate’s boltgun can be replaced with one of the following:<br><ul><li>1 power weapon</li><li>1 power fist</li><li>1 heavy bolter</li><li>1 heavy flamer</li><li>1 grav-cannon</li><li>1 lascannon</li><li>1 missile launcher</li><li>1 multi-melta</li><li>1 plasma cannon</li></ul>"
    },
    {
      "datasheetId": "000004154",
      "line": 6,
      "button": "*",
      "description": "*This model can only be equipped with two ranged weapons if one of them is a Pistol (and it can only have one Pistol)."
    }
  ],
